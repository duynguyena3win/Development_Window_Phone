<UserControl x:Class="AppboyUI.Phone.Controls.StarRating"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  mc:Ignorable="d"
  FontFamily="{StaticResource PhoneFontFamilyNormal}"
  FontSize="{StaticResource PhoneFontSizeNormal}"
  Foreground="{StaticResource PhoneForegroundBrush}"
  d:DesignWidth="100" d:DesignHeight="20">

  <StackPanel x:Name="StarStackPanel" Orientation="Horizontal">
    <Image Source="\Assets\Images\Star\Blue\com_appboy_star_empty.png" Style="{StaticResource Appboy.StarRating.Image}"/>
    <Image Source="\Assets\Images\Star\Blue\com_appboy_star_empty.png" Style="{StaticResource Appboy.StarRating.Image}"/>
    <Image Source="\Assets\Images\Star\Blue\com_appboy_star_empty.png" Style="{StaticResource Appboy.StarRating.Image}"/>
    <Image Source="\Assets\Images\Star\Blue\com_appboy_star_empty.png" Style="{StaticResource Appboy.StarRating.Image}"/>
    <Image Source="\Assets\Images\Star\Blue\com_appboy_star_empty.png" Style="{StaticResource Appboy.StarRating.Image}"/>
  </StackPanel>
</UserControl>